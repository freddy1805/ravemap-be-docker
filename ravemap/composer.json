{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^1.13",
        "doctrine/common": "^v2.8.1",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.7.5",
        "endroid/qr-code-bundle": "3.4.3",
        "friendsofsymfony/user-bundle": "^2.1",
        "gesdinet/jwt-refresh-token-bundle": "^0.12.0",
        "gos/web-socket-bundle": "^v3.11.0",
        "jms/serializer-bundle": "3.7.0",
        "lexik/jwt-authentication-bundle": "^2.12",
        "nelmio/api-doc-bundle": "4.0.1",
        "redjanym/fcm-bundle": "^1.1",
        "sonata-project/admin-bundle": "^3.104",
        "sonata-project/core-bundle": "3.*",
        "sonata-project/doctrine-orm-admin-bundle": "^3.35",
        "sonata-project/media-bundle": "^3.13",
        "symfony/asset": "4.4.*",
        "symfony/cache": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/event-dispatcher": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/messenger": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/translation": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.33",
        "symfony/stopwatch": "^4.4",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
